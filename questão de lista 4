#include <stdio.h>
#define MAX 5

int main(){
    
    int V[MAX],i;
    
    for(i=0;i<MAX;i++){
    printf("escreva um numero para a posicao V[%d]\n",i);
    scanf("%d",&V[i]); }
    
    printf("Numeros escolhidos:\n");
    for(i=0;i<MAX;i++)
    printf("%d ",V[i]);
    
    
    printf("\n");
    
    printf("resposta:\n");
    
    for(i=0;i<MAX;i++)
    if(V[i]>=0){
        
        printf("%d ",V[i]);
        
    }
    
}
